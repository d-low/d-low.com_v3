{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "react/jsx-closing-bracket-location": [2, {
      "nonEmpty": "after-props",
      "selfClosing": false
    }],
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": [2, {"ignorePureComponents": true}]
  }
}
